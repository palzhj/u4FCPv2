#Pin LOC constraints for the status signals init_calib_complete and data_compare_error

#LOC constraints provided if the pins are selected for status signals

# Blue LED low active, Not finish -> blue
set_property PACKAGE_PIN C10 [ get_ports "c0_init_calib_complete" ]
set_property IOSTANDARD LVCMOS33 [ get_ports "c0_init_calib_complete" ]
set_property DRIVE 8 [ get_ports "c0_init_calib_complete" ]

# Green LED low active, No error -> green
set_property PACKAGE_PIN C7 [ get_ports "c0_data_compare_error" ]
set_property IOSTANDARD LVCMOS33 [ get_ports "c0_data_compare_error" ]
set_property DRIVE 8 [ get_ports "c0_data_compare_error" ]

# 125 MHz
set_property PACKAGE_PIN AU32 [ get_ports "c0_sys_clk_p" ]
set_property IOSTANDARD LVDS [ get_ports "c0_sys_clk_p" ]
set_property PACKAGE_PIN AU33 [ get_ports "c0_sys_clk_n" ]
set_property IOSTANDARD LVDS [ get_ports "c0_sys_clk_n" ]

# DIPSW[0]
set_property PACKAGE_PIN D12 [ get_ports "sys_rst" ]
set_property IOSTANDARD LVCMOS25 [ get_ports "sys_rst" ]
set_property PULLUP     true     [get_ports "sys_rst"]

